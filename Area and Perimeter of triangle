class myTriangle {
  int a = 0;
  int b = 0;
 int c = 0;

  void areaTriangle(int a, int b) {
    this.a = a;
    this.b = b;
    double area = (a * b) / 2;
    print("Area of triangle is: $area");
  }

  void perimeterTriangle(int a , int b, int c) {
  int perimeter = (a + b + c);
   print("Perimeter of triangle is:  $perimeter");
  }
}

  void main() {
    myTriangle t1 = myTriangle();
    t1.areaTriangle(5, 2);
    t1.perimeterTriangle(5,2,2);
  }

