class myTable{
  int n=0;
  
  
  void printTable()
  {
    for(int i =1; i<=10;i++){
      print('$n X $i = ${n*i}');
    }
    
  }
  void myNumber(int x){
    n = x;
      printTable();
  }
}


void main()
{
  myTable m1=new myTable();
  m1.myNumber(52);
  m1.myNumber(55);
  m1.myNumber(5);
  
}
